<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.bustogoapplictionv3.Mainmenu">

    <ImageView
        android:id="@+id/imva"
        android:layout_width="387dp"
        android:layout_height="138dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="-11dp"
        android:background="#9bd6d4"
        android:onClick="imva"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/busonenikekek" />

    <ImageView
        android:id="@+id/imvb"
        android:layout_width="387dp"
        android:layout_height="151dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:background="#ff9010"
        android:onClick="imvb"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imva"
        app:srcCompat="@drawable/roadorforfun" />

    <ImageView
        android:id="@+id/imvc"
        android:layout_width="394dp"
        android:layout_height="152dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="-47dp"
        android:background="#abd938"
        android:onClick="imvc"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imvb"
        app:srcCompat="@drawable/uiui" />

    <ImageView
        android:id="@+id/imvd"
        android:layout_width="391dp"
        android:layout_height="133dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:background="#a9aba2"
        android:onClick="imvd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imvc"
        app:layout_constraintVertical_bias="0.61"
        app:srcCompat="@drawable/newsone" />
</android.support.constraint.ConstraintLayout>
